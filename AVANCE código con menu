#include <stdio.h>
#define CAPRICORNIO 1
#define ACUARIO 2
#define PISCIS 3
#define ARIES 4
#define TAURO 5
#define GEMINIS 6
#define CANCER 7
#define LEO 8
#define VRIGO 9
#define LIBRA 10
#define ESCROPIO 11
#define SAGITARIO 12

int main(){
int signo;

    printf("Bienvenido\nSeguí los siguientes pasos:");
    printf("\n1- Busca en qué signo entra la fecha de tu cumpleaños\n2- Ingresa el número asignado a su derecha para obtener información\n");
    printf("\nCapricornio(22/12 al 19/01) = 1\nAcuario(20/12 al 18/01) = 2\nPiscis(19/02 al 19/03 = 3\nAries(20/03 al 18/04) = 4\nTauro(19/04 al 20/05) = 5\nGéminis(21/05 al 20/06) = 6\nCáncer(21/06 al 22/07 = 7\nLeo(23/07 al 22/08) = 8\nVirgo(23/08 al 22/09) = 9\nLibra(23/09 al 22/10) = 10\nEscorpio(23/10 al 21/11) = 11\nSagitario(22/11 al 21/12) = 12\n");
    scanf("%d" , &signo);
   
    return 0;
}
